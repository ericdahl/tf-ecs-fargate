[
  {
    "name": "splunk",
    "image": "splunk/splunk:latest",
    "portMappings": [
      {
        "containerPort": 8000,
        "protocol": "tcp"
      },
      {
        "containerPort": 8088,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "SPLUNK_START_ARGS",
        "value": "--accept-license"
      },
      {
        "name": "SPLUNK_PASSWORD",
        "value": "password"
      }
    ]
  }
]